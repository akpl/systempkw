    <property name="connection.url">jdbc:mysql://wybory.elential.com:3306/wybory?useSSL=false</property>
    <property name="connection.driver_class">com.mysql.jdbc.Driver</property>
    <mapping class="pkw.models.PoziomDostepu"/>
    <mapping class="pkw.models.Uzytkownik"/>
    <mapping class="pkw.models.TypWyborow"/>
    <mapping class="pkw.models.Wybory"/>
    <mapping class="pkw.models.KandydatPosel"/>
    <mapping class="pkw.models.KandydatPrezydent"/>
    <mapping class="pkw.models.Komitet"/>
    <mapping class="pkw.models.PytanieReferendalne"/>
    <mapping class="pkw.models.WynikiPosel"/>
    <mapping class="pkw.models.WynikiPrezydent"/>
    <mapping class="pkw.models.WynikiPytaniaReferendalne"/>
    <mapping class="pkw.models.Komisja"/>
    <mapping class="pkw.models.Okreg"/>
